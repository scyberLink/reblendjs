{
  "name": "reblendjs",
  "version": "9.2.2",
  "codename": "infant",
  "description": "ReblendJs uses Reactjs pradigm to build UI components, with isolated state for each components.",
  "keywords": [
    "ReblendJs",
    "Reative",
    "UI",
    "Library"
  ],
  "main": "lib/index.js",
  "types": "src/index.ts",
  "files": [
    "LICENSE",
    "HISTORY.md",
    "README.md",
    "src",
    "lib",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scyberLink/reblendjs.git",
    "directory": "packages/reblendjs"
  },
  "scripts": {
    "dev": "tsc && babel dist --out-dir lib",
    "build": "rm -rf dist; rm -rf lib; tsc && babel dist --out-dir lib",
    "format": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:autofix": "eslint ./src --fix",
    "prettier": "prettier ./src",
    "prepublishOnly": "node ./setReblendVersion.js",
    "prettier:fix": "prettier --write ./src"
  },
  "author": "Emmanuel Elom <scansioquielom@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "babel-preset-reblend": "latest",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "prettier": "^3.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^7.6.0"
  },
  "dependencies": {
    "reblend-typing": "latest"
  },
  "gitHead": "79a3e22f73913a686e76631559ba7830f7eb2e98"
}
